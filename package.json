{
  "name": "andri.dk",
  "version": "1.0.0",
  "author": "Andri Oskarsson <andri@andri.dk>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "gatsby build",
    "build:oldschool": "gatsby build",
    "dev": "gatsby develop -H 0.0.0.0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-cv": "cd src/cv && babel-node build.js",
    "watch-cv": "cd src/cv && nodemon --exec babel-node build.js",
    "build-card": "cd plugins/social-card && babel-node build.js",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "devDependencies": {
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "cacheDirectories": [
    ".cache",
    "public"
  ]
}
