{
  "name": "andri.dk",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "dev:node": "bunx astro dev",
    "start": "node server.mjs",
    "build": "astro build",
    "format": "prettier --write .",
    "verify:formatting": "prettier . --check",
    "verify:types": "tsc --noEmit",
    "preview": "bunx astro preview",
    "deno": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "check": "astro check",
    "build:bin": "bun build --compile --packages external --outfile ./app server.mjs",
    "bundle": "bun build --target bun --bundle --packages external --outfile ./dist/weird-bun-bundle server.mjs"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/node": "^9.4.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/tailwind": "6.0.2",
    "@iconify-json/lucide": "^1.2.20",
    "@iconify-json/simple-icons": "^1.2.17",
    "@iconify-json/tabler": "^1.2.13",
    "@react-pdf/renderer": "^4",
    "astro": "^5.12.9",
    "astro-icon": "^1.1.5",
    "express": "^5",
    "html2canvas": "^1.4.1",
    "markdown-it": "^14",
    "openai": "^5",
    "react": "^19",
    "react-dom": "^19",
    "remark-embed-images": "^5",
    "tailwind-merge": "^3",
    "typescript": "^5.7.2",
    "@fontsource-variable/inter": "^5.1.1",
    "@fontsource-variable/montserrat": "^5.2",
    "@fontsource-variable/source-code-pro": "^5.1.1",
    "@fontsource/inter": "^5.1.1",
    "@fontsource/montserrat": "^5.0.20",
    "simple-icons": "^15"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@tailwindcss/typography": "~0.5.15",
    "@types/bun": "^1.1.14",
    "@types/markdown-it": "^14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.13.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "~0.14.1",
    "sanitize-html": "^2.14.0",
    "tailwindcss": "~3.4.17",
    "vfile": "^6.0.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
