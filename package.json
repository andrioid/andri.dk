{
  "name": "andri.dk",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "node server.mjs",
    "build": "astro build",
    "format": "prettier --write .",
    "verify:formatting": "prettier . --check",
    "verify:types": "tsc --noEmit",
    "preview": "astro preview",
    "md-social": "md-social parse src/content/blog",
    "deno": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "check": "astro check",
    "build:bin": "bun build --compile --packages external --outfile ./app server.mjs",
    "bundle": "bun build --target bun --bundle --packages external --outfile ./dist/weird-bun-bundle server.mjs",
    "upgrade": "npx @astrojs/upgrade"
  },
  "dependencies": {
    "@andrioid/md-social": "^0.0.1-053d1d0",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/node": "^9.5.1",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@fontsource-variable/inter": "^5.1.1",
    "@fontsource-variable/montserrat": "^5.2",
    "@fontsource-variable/source-code-pro": "^5.1.1",
    "@fontsource/inter": "^5.1.1",
    "@fontsource/montserrat": "^5.0.20",
    "@iconify-json/lucide": "^1.2.20",
    "@iconify-json/simple-icons": "^1.2.17",
    "@iconify-json/tabler": "^1.2.13",
    "@react-pdf/renderer": "^4",
    "astro": "^5.16.2",
    "astro-icon": "^1.1.5",
    "express": "^5",
    "html2canvas": "^1.4.1",
    "markdown-it": "^14",
    "openai": "^5",
    "react": "^19",
    "react-dom": "^19",
    "remark-embed-images": "^5",
    "simple-icons": "^15",
    "tailwind-merge": "^3",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bun": "^1.1.14",
    "@types/markdown-it": "^14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.13.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "~0.14.1",
    "sanitize-html": "^2.14.0",
    "tailwindcss": "~4.1.17",
    "vfile": "^6.0.3"
  },
  "workspaces": [
    "packages/*"
  ]
